{
  "name": "<%= appname %>",
  "description": "<%= packageDescription %>",
  "repository": {
    "type": "<%= packageRepositoryType %>",
    "url": "<%= packageRepositoryUrl %>"
  },
  "version": "<%= packageVersion %>",
  "license": "<%= packageLicense %>",
  "main": "bundles/<%= appname %>.umd.min.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\" && npm run tslint \"server/**/*.ts\"",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "umd": "rm -rf bundles && webpack --config config/webpack.umd.js --progress --profile --bail",
    "clean": "rm -rf docs",
    "start": "npm run server:dev:hmr",
    "ngc": "ngc"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/compiler-cli": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/forms": "^2.4.6",
    "@angular/platform-browser": "^2.4.6",
    "@angular/platform-browser-dynamic": "^2.4.6",
    "@angular/router": "^3.4.6",
    "@types/core-js": "^0.9.35",
    "@types/node": "^7.0.5",
    "@types/webpack": "^2.2.5",
    "angular2-template-loader": "^0.6.1",
    "ts-loader": "~2.0.0",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.28.0",
    "raw-loader": "^0.5.1",
    "rxjs": "^5.1.0",
    "style-loader": "^0.13.1",
    "tslint": "^4.4.2",
    "typescript": "2.1.5",
    "typescript-formatter": "^4.1.0",
    "webpack": "~2.2.1",
    "webpack-dev-server": "~2.3.0",
    "webpack-merge": "^2.6.1",
    "zone.js": "^0.7.6"
  }
}
